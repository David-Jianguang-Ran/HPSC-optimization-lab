# we are only keeping one
mpirun -n 4 valgrind --tool=cachegrind --cachegrind-out-file="cachegrind.wip.out" ./esPIC-original -nPEx 2 -nPEy 2 -nCellx 40 -nCelly 40 -nPtcl 100000  -flux 1000  -tEnd 1 -dt .01 -tPlot .2 -noPlot > wip-cache.out
mpirun -n 4 valgrind --tool=callgrind --callgrind-out-file="callgrind.wip.out" ./esPIC-original -nPEx 2 -nPEy 2 -nCellx 40 -nCelly 40 -nPtcl 100000  -flux 1000  -tEnd 1 -dt .01 -tPlot .2 -noPlot > wip-call.out

callgrind_annotate --auto=yes cachegrind.wip.out >  cachegrind.wip.txt